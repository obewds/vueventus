{
  "name": "@obewds/vueventus",
  "description": "Aesthetically flexible components for modern Vue.js, Tailwind CSS, and Typescript apps",
  "version": "0.15.3",
  "author": "@oberocks",
  "homepage": "https://obewds.github.io/vueventus/",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "ts:build": "npx rimraf dist && npm run build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "test": "vitest --dom",
    "coverage": "vitest run --dom --coverage",
    "docs:dev": "vitepress dev docs --open",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "changelog": "auto-changelog --package --unreleased --backfill-limit 20",
    "tag": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
    "bump": "npm run tag && npm run changelog",
    "cleanup": "node scripts/cleanup.mjs",
    "originals": "node scripts/copyToDist.mjs",
    "generate": "npx rimraf dist && npx rimraf docs/.vitepress/dist && npm run build && vue-tsc --emitDeclarationOnly && tsc && npm run docs:build && npm run cleanup && npm run originals",
    "pkg:org": "npm config set scope obewds",
    "pkg:login": "npm adduser",
    "pkg:publish": "npm publish --access public",
    "publish:package": "npm run generate && npm run pkg:org && npm run pkg:login && npm run pkg:publish && npm run bump"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "deepmerge": "^4.2.2",
    "tailwindcss": "^3.1.3",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "^2.0.0",
    "auto-changelog": "^2.4.0",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.3",
    "chalk": "^5.0.1",
    "fs-extra": "^10.1.0",
    "happy-dom": "^5.3.1",
    "markdown-it-include": "^2.0.0",
    "postcss": "^8.4.14",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vitepress": "^1.0.0-alpha.2",
    "vitest": "^0.15.1",
    "vue-router": "^4.0.16",
    "vue-tsc": "^0.37.9"
  },
  "sideEffects": false,
  "main": "./dist/vueventus.umd.js",
  "module": "./dist/vueventus.es.js",
  "browser": "./dist/vueventus.iife.js",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vueventus.es.js",
      "require": "./dist/vueventus.umd.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./components/*": "./dist/components/*",
    "./anchors/*": "./dist/components/anchors/*",
    "./buttons/*": "./dist/components/buttons/*",
    "./elements/*": "./dist/components/elements/*",
    "./inputs/*": "./dist/components/inputs/*",
    "./logos/*": "./dist/components/logos/*",
    "./configs/*": "./dist/configs/*",
    "./data/*": "./dist/data/*",
    "./helpers/*": "./dist/helpers/*",
    "./palettes/*": "./dist/configs/palettes/*",
    "./types/*": "./dist/types/*",
    "./validators/*": "./dist/validators/*",
    "./VvAnchor.vue": "./dist/components/anchors/VvAnchor.vue",
    "./VvButton.vue": "./dist/components/buttons/VvButton.vue",
    "./VvColorModeButton.vue": "./dist/components/buttons/VvColorModeButton.vue",
    "./VvEl.vue": "./dist/components/elements/VvEl.vue",
    "./VvInput.vue": "./dist/components/inputs/VvInput.vue",
    "./VueVentusLogoText.vue": "./dist/components/logos/VueVentusLogoText.vue",
    "./VueVentusSpinningMark.vue": "./dist/components/logos/VueVentusSpinningMark.vue",
    "./Anchors.js": "./dist/configs/Anchors.js",
    "./Buttons.js": "./dist/configs/Buttons.js",
    "./Inputs.js": "./dist/configs/Inputs.js",
    "./Text.js": "./dist/configs/Text.js",
    "./Transitions.js": "./dist/configs/Transitions.js",
    "./VvConfig.js": "./dist/configs/VvConfig.js",
    "./app.color.data.json": "./dist/data/app.color.data.json",
    "./extend.animation.json": "./dist/data/extend.animation.json",
    "./extend.keyframes.json": "./dist/data/extend.keyframes.json",
    "./extend.maxWidth.json": "./dist/data/extend.maxWidth.json",
    "./extend.rotate.json": "./dist/data/extend.rotate.json",
    "./extend.scale.json": "./dist/data/extend.scale.json",
    "./extend.transitionDuration.json": "./dist/data/extend.transitionDuration.json",
    "./extend.width.json": "./dist/data/extend.width.json",
    "./extend.fontSize.json": "./dist/data/theme.fontSize.json",
    "./extend.listStyleType.json": "./dist/data/theme.listStyleType.json",
    "./extend.opacity.json": "./dist/data/theme.opacity.json",
    "./extend.screens.json": "./dist/data/theme.screens.json",
    "./camelCaseToTitleCase.js": "./dist/helpers/camelCaseToTitleCase.js",
    "./digitsOnly.js": "./dist/helpers/digitsOnly.js",
    "./formatBytes.js": "./dist/helpers/formatBytes.js",
    "./formatMediaTime.js": "./dist/helpers/formatMediaTime.js",
    "./formatNumber.js": "./dist/helpers/formatNumber.js",
    "./mergeWithVvConfig.js": "./dist/helpers/mergeWithVvConfig.js",
    "./randomString.js": "./dist/helpers/randomString.js",
    "./slugifyString.js": "./dist/helpers/slugifyString.js",
    "./stringToCamelCase.js": "./dist/helpers/stringToCamelCase.js",
    "./stringToFilename.js": "./dist/helpers/stringToFilename.js",
    "./uniqueArray.js": "./dist/helpers/uniqueArray.js",
    "./AnchorDefault.js": "./dist/configs/palettes/AnchorDefault.js",
    "./BorderDefault.js": "./dist/configs/palettes/BorderDefault.js",
    "./ButtonOutline.js": "./dist/configs/palettes/ButtonOutline.js",
    "./ButtonSolid.js": "./dist/configs/palettes/ButtonSolid.js",
    "./GroundConsole.js": "./dist/configs/palettes/GroundConsole.js",
    "./GroundDefault.js": "./dist/configs/palettes/GroundDefault.js",
    "./GroundMonochromatic.js": "./dist/configs/palettes/GroundMonochromatic.js",
    "./GroundPastel.js": "./dist/configs/palettes/GroundPastel.js",
    "./InputValidation.js": "./dist/configs/palettes/InputValidation.js",
    "./TextDefault.js": "./dist/configs/palettes/TextDefault.js",
    "./ValidAudioSourceTypes.js": "./dist/validators/ValidAudioSourceTypes.js",
    "./ValidButtonTypes.js": "./dist/validators/ValidButtonTypes.js",
    "./ValidColorModes.js": "./dist/validators/ValidColorModes.js",
    "./ValidDirections.js": "./dist/validators/ValidDirections.js",
    "./ValidElementTags.js": "./dist/validators/ValidElementTags.js",
    "./ValidFontAwesomeFamilies.js": "./dist/validators/ValidFontAwesomeFamilies.js",
    "./ValidFontAwesomeSizes.js": "./dist/validators/ValidFontAwesomeSizes.js",
    "./ValidHeadingLevels.js": "./dist/validators/ValidHeadingLevels.js",
    "./ValidImageSourceTypes.js": "./dist/validators/ValidImageSourceTypes.js",
    "./ValidInputTypes.js": "./dist/validators/ValidInputTypes.js",
    "./ValidVideoSourceTypes.js": "./dist/validators/ValidVideoSourceTypes.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obewds/vueventus.git"
  },
  "bugs": {
    "url": "https://github.com/obewds/vueventus/issues"
  },
  "keywords": [
    "vue",
    "vueventus",
    "vite",
    "tailwindcss",
    "obewds",
    "rollup",
    "vitepress",
    "postcss",
    "nodejs"
  ]
}
